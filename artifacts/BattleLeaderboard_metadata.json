		"devdoc": {
			"details": "Manages battle predictions, results, and player statistics",
			"kind": "dev",
			"methods": {
				"emergencyWithdraw()": {
					"details": "Emergency function to withdraw funds (if any)"
				},
				"getAccuracy(address)": {
					"details": "Get accuracy percentage for a player",
					"params": {
						"player": "The player's address"
					},
					"returns": {
						"_0": "uint256 Accuracy as percentage (0-100)"
					}
				},
				"getBattleById(uint256)": {
					"details": "Get battle record by ID",
					"params": {
						"battleId": "The ID of the battle"
					},
					"returns": {
						"_0": "BattleRecord struct for the specified battle"
					}
				},
				"getBattleCount()": {
					"details": "Get total number of battles recorded",
					"returns": {
						"_0": "uint256 Total battle count"
					}
				},
				"getPlayerBattleCount(address)": {
					"details": "Get number of battles for a player",
					"params": {
						"player": "The player's address"
					},
					"returns": {
						"_0": "uint256 Number of battles"
					}
				},
				"getPlayerBattleIds(address)": {
					"details": "Get all battle IDs for a player",
					"params": {
						"player": "The player's address"
					},
					"returns": {
						"_0": "uint256[] Array of battle IDs"
					}
				},
				"getUserStats(address)": {
					"details": "Get complete statistics for a player",
					"params": {
						"user": "The player's address"
					},
					"returns": {
						"_0": "PlayerStats struct containing all stats"
					}
				},
				"submitBattle(string,string,string,string,uint256,uint256,uint256)": {
					"details": "Submit a battle result and update player statistics",
					"params": {
						"actualWinner": "Actual battle result",
						"coinA": "First cryptocurrency symbol",
						"coinB": "Second cryptocurrency symbol",
						"performanceDelta": "Performance delta in basis points",
						"predictedWinner": "User's prediction",
						"scoreA": "Score of coin A",
						"scoreB": "Score of coin B"
					},
					"returns": {
						"_0": "battleId The ID of the recorded battle",
						"_1": "wasCorrect Whether the prediction was correct",
						"_2": "totalPointsEarned Points earned from this battle"
					}
				},
				"wasPredictionCorrect(uint256,address)": {
					"details": "Compare if a player's prediction was correct",
					"params": {
						"battleId": "The battle ID to check",
						"player": "The player to verify"
					},
					"returns": {
						"_0": "bool Whether the prediction was correct"
					}
				}
			},
			"title": "BattleLeaderboard",
			"version": 1
		},
